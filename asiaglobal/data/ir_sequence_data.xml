<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_job_order" model="ir.sequence">
            <field name="name">Job Order</field>
            <field name="code">asiaglobal.job.order</field>
            <field name="prefix">AGT-JO-%(range_year)s-</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_service_report" model="ir.sequence">
            <field name="name">Service Report</field>
            <field name="code">asiaglobal.service.report</field>
            <field name="prefix"></field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_job_material_request" model="ir.sequence">
            <field name="name">Job / Material Request\</field>
            <field name="code">asiaglobal.job.material.request</field>
            <field name="prefix"></field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
